# setup Visual C++ .Net
#
# setup for use with DOS tools
export VCINSTALLDIR="d:/Program Files/Microsoft Visual Studio .NET"
export VSCOMNTOOLS="$VCINSTALLDIR/Common7/Tools/"
export VSINSTALLDIR="$VCINSTALLDIR/Common7/IDE"
VCINCLUDE1="$VCINSTALLDIR/VC7/ATLMFC/INCLUDE"
VCINCLUDE2="$VCINSTALLDIR/VC7/INCLUDE"
VCINCLUDE3="$VCINSTALLDIR/VC7/PlatformSDK/include/prerelease"
VCINCLUDE4="$VCINSTALLDIR/VC7/PlatformSDK/include"
VCINCLUDE5="$VCINSTALLDIR/FrameworkSDK/include/"
export VCINCLUDE="$VCINCLUDE1;$VCINCLUDE2;$VCINCLUDE3;$VCINCLUDE4;$VCINCLUDE5"
VCLIB1="$VCINSTALLDIR/VC7/ATLMFC/LIB"
VCLIB2="$VCINSTALLDIR/VC7/LIB"
VCLIB3="$VCINSTALLDIR/VC7/PlatformSDK/lib/prerelease"
VCLIB4="$VCINSTALLDIR/VC7/PlatformSDK/lib"
VCLIB5="$VCINSTALLDIR/FrameworkSDK/Lib"
export VCLIB="$VCLIB1;$VCLIB2;$VCLIB3;$VCLIB4;$VCLIB5"
VCPATH="$VSINSTALLDIR;$VCINSTALLDIR/VC7/BIN;$VSCOMNTOOLS;$VSCOMNTOOLS/bin/prerelease;$VSCOMNTOOLS/bin;$VCINSTALLDIR/FrameworkSDK/bin;C:/WINNT/Microsoft.NET/Framework/v1.0.3705;C:/WINNT/system32;C:/WINNT;C:/WINNT/System32/Wbem;C:/WINNT"
PATH="$VCPATH;$PATH"
